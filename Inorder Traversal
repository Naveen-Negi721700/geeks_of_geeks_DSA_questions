/*
class Node {
  public:
    int data;
    Node* left;
    Node* right;

    Node(int val) {
        data = val;
        left = NULL;
        right = NULL;
    }
};
*/

class Solution {
  public:
  void helper(Node* root,vector<int>&ans)
  {
      if(root==NULL)
      {
          return ;
      }
      helper(root->left,ans);
      ans.push_back(root->data);
      helper(root->right,ans);
  }
    vector<int> inOrder(Node* root) {
        // code here
    vector<int>ans;
    helper(root,ans);
    return ans;
        
    }
};
