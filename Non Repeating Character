class Solution {
  public:
    char nonRepeatingChar(string &s) {
        //  code here
        int n=s.length();
        unordered_map<char,int>m;
        for(int i=0;i<n;i++)
        {
            m[s[i]]++;
        }
        for(int i=0;i<n;i++)
        {
            if(m[s[i]]==1)
            {
                return s[i];
            }
        }
        return '$';
    }
};
