/*
Node is as follows
class Node {
    int data;
    Node *left;
    Node *right;

    Node(int val) {
        data = val;
        left = right = NULL;
    }
};
*/

class Solution {
  public:
  bool helper(Node *root, int target,unordered_set<int>&st)
  {
      if (root==NULL)
      {
          return false;
      }
      if(st.find(target-root->data)!=st.end()){
          return true;
      }
      st.insert(root->data);
    return helper(root->left,target,st)||helper(root->right,target,st);
  }
    bool findTarget(Node *root, int target) {
        // your code here.
        unordered_set<int>st;
       return  helper(root,target,st);
    }
};
