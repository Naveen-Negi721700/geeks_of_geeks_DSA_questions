class Solution {
  public:
    int findUnique(vector<int> &arr) {
        // code here
        unordered_map<int,int>m;
        for(int i:arr)
        {
            m[i]++;
        }
        for(auto &i:m)
        {
            if(i.second==1)
            {
                return i.first;
            }
        }
        return -1;
    }
};
