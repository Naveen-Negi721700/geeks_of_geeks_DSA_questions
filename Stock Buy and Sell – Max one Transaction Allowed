class Solution {
  public:
    int maximumProfit(vector<int> &prices) {
        // code here
        {
            int n=prices.size();
    // code here
    int profit=0;
    int buy=prices[0];
    
    for(int i=1;i<n;i++)
    {
        if(prices[i]>buy)
        {
            profit=max(profit,prices[i]-buy);
        }
        else{
            buy=prices[i];
        }
    }
    if(profit==0)
    {
        return 0;
    }
    return profit;
        }
    }
};
