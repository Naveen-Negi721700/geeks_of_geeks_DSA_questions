class Solution {
  public:
    int lis(vector<int>& arr) {
    
        vector<int>lis;
        for(int x:arr)
        {
            auto it=lower_bound(lis.begin(),lis.end(),x);
            if(it==lis.end())
            lis.push_back(x);
            else *it=x;
        }
        return lis.size();
    }
};
