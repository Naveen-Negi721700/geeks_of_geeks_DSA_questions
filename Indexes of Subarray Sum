class Solution {
  public:
    vector<int> subarraySum(vector<int> &arr, int target) {
        int n=arr.size();
        int sum=0;
        int j=0;
        for(int i=0;i<n;i++)
        {
           sum=sum+arr[i];
           while(sum>target)
           {
              sum=sum-arr[j];
              j++;
           }
           if(sum==target)
           {
               return {j+1,i+1};
           }
        }
        return {-1};
    }
};
